{
    "name": "prettier-plugin-multiline-arrays",
    "version": "1.1.2",
    "description": "Prettier plugin to force all arrays to be multiline.",
    "keywords": [
        "array",
        "format",
        "multiline",
        "plugin",
        "prettier",
        "printer"
    ],
    "homepage": "https://github.com/electrovir/prettier-plugin-multiline-arrays",
    "bugs": {
        "url": "https://github.com/electrovir/prettier-plugin-multiline-arrays/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/electrovir/prettier-plugin-multiline-arrays"
    },
    "license": "MIT",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "compile": "rm -rf dist && virmator compile",
        "debug": "cross-env NEW_LINE_DEBUG=true npm run test",
        "docs:update": "virmator code-in-markdown",
        "format": "virmator format",
        "publish": "virmator publish \"npm run compile && npm run test:all\"",
        "test": "npm run compile && rm -rf node_modules/prettier-plugin-multiline-arrays && cp -r dist node_modules/prettier-plugin-multiline-arrays && virmator test",
        "test:all": "npm run test:types && npm run test:coverage && npm run test:spelling && npm run test:format && npm run test:docs",
        "test:coverage": "virmator test coverage",
        "test:docs": "virmator code-in-markdown check",
        "test:format": "prettier --color \"./**/*.+(cjs|css|graphql|html|js|json|jsx|less|md|mjs|scss|toml|ts|tsx|yaml|yml)\" --check",
        "test:spelling": "virmator spellcheck",
        "test:types": "tsc --noEmit"
    },
    "dependencies": {
        "@augment-vir/common": "^8.1.0"
    },
    "devDependencies": {
        "@electrovir/nyc": "^15.1.0-fix0",
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.4",
        "@types/esprima": "^4.0.3",
        "@types/mocha": "^10.0.1",
        "@types/prettier": "^2.7.2",
        "chai": "^4.3.7",
        "cross-env": "^7.0.3",
        "cspell": "^6.18.1",
        "istanbul-smart-text-reporter": "^1.1.1",
        "markdown-code-example-inserter": "^0.1.13",
        "mocha": "^10.2.0",
        "mocha-spec-reporter-with-file-names": "^0.0.3",
        "prettier": "^2.8.2",
        "prettier-plugin-jsdoc": "^0.4.2",
        "prettier-plugin-multiline-arrays": "*",
        "prettier-plugin-organize-imports": "^3.2.2",
        "prettier-plugin-packagejson": "^2.3.0",
        "prettier-plugin-sort-json": "^1.0.0",
        "prettier-plugin-toml": "^0.3.1",
        "ts-node": "^10.9.1",
        "typescript": "4.9.4",
        "virmator": "^5.3.2"
    }
}
